<?xml version="1.0"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd"
         xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>

  <parent>
    <groupId>pentaho</groupId>
    <artifactId>pentaho-osgi-bundles</artifactId>
    <version>8.3.0.0-SNAPSHOT</version>
  </parent>

  <name>Pentaho Process Governor</name>

  <groupId>org.hitachivantara</groupId>
  <artifactId>pentaho-proc-governor</artifactId>
  <version>8.3.0.0-SNAPSHOT</version>

  <packaging>pom</packaging>

  <modules>
    <module>impl</module>
  </modules>

  <properties>
    <javax.servlet.version>3.0.1</javax.servlet.version>
    <commons-lang.version>2.6</commons-lang.version>
    <bndlib.version>2.4.0</bndlib.version>
  </properties>

  <dependencyManagement>
    <dependencies>
      <dependency>
        <groupId>javax.servlet</groupId>
        <artifactId>javax.servlet-api</artifactId>
        <version>${javax.servlet.version}</version>
        <exclusions>
          <exclusion>
            <groupId>*</groupId>
            <artifactId>*</artifactId>
          </exclusion>
        </exclusions>
      </dependency>

      <dependency>
        <groupId>commons-lang</groupId>
        <artifactId>commons-lang</artifactId>
        <version>${commons-lang.version}</version>
        <exclusions>
          <exclusion>
            <groupId>*</groupId>
            <artifactId>*</artifactId>
          </exclusion>
        </exclusions>
      </dependency>
    </dependencies>
  </dependencyManagement>



</project>
